        </activity>
        <activity android:name=".VigenereActivity" android:parentActivityName=".CaesarActivity">

        </activity>
        <activity android:name=".RailfenceActivity" android:parentActivityName=".CaesarActivity">

        </activity>
    </application>

</manifest>